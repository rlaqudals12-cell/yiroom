{
  "name": "@yiroom/mobile",
  "version": "0.1.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "echo 'Mobile build requires EAS'",
    "build:dev": "eas build --profile development --platform all",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:production": "eas build --profile production --platform all",
    "build:local:android": "eas build --profile development --platform android --local",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/",
    "test:e2e:android": "maestro test .maestro/ --device android",
    "test:e2e:ios": "maestro test .maestro/ --device ios",
    "test:e2e:record": "maestro record .maestro/"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@supabase/supabase-js": "^2.49.8",
    "@yiroom/shared": "*",
    "expo": "~54.0.25",
    "expo-camera": "~17.0.9",
    "expo-constants": "^18.0.10",
    "expo-dev-client": "^6.0.18",
    "expo-haptics": "^15.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "^8.0.9",
    "expo-notifications": "^0.32.13",
    "expo-router": "^6.0.15",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "^6.0.8",
    "lucide-react-native": "^0.562.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.15.1",
    "tailwindcss": "^3.4.19",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  }
}
