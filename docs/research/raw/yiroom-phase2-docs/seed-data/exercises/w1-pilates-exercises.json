  "principles": [
    {
      "id": 1,
      "name_en": "Concentration",
      "name_kr": "집중",
      "definition": "신체와 정신을 완전히 연결하여 각 동작에 전적인 주의를 기울이는 것",
      "application": ["외부 자극 차단", "활성화 근육 인식", "신체 피드백 반응"]
    },
    {
      "id": 2,
      "name_en": "Control",
      "name_kr": "조절",
      "definition": "의식적이고 신중한 근육 조절로 모든 동작 수행",
      "application": ["관성 의존 없이 근육 힘으로 동작", "모든 단계 의식적 조절"]
    },
    {
      "id": 3,
      "name_en": "Centering",
      "name_kr": "중심화",
      "definition": "파워하우스에서 모든 움직임 시작",
      "application": ["코어에서 시작하여 사지로 확장", "배꼽을 척추 쪽으로"]
    },
    {
      "id": 4,
      "name_en": "Flow",
      "name_kr": "흐름",
      "definition": "운동이 부드럽고 유려하게 연결",
      "application": ["동작 사이 멈춤 없이 전환", "호흡 리듬에 맞춤"]
    },
    {
      "id": 5,
      "name_en": "Precision",
      "name_kr": "정확성",
      "definition": "올바른 배치, 정렬, 궤적에 지속적 주의",
      "application": ["반복 횟수보다 품질 집중", "세부사항까지 주의"]
    },
    {
      "id": 6,
      "name_en": "Breathing",
      "name_kr": "호흡",
      "definition": "측면 호흡으로 코어 활성화 유지하며 깊은 호흡",
      "application": ["들숨-갈비뼈 확장", "날숨-복부 수축", "노력 동작 시 내쉬기"]
    }
  ],
